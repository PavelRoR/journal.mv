@import '../css/_css/jquery.fancybox.min.css'



*
	margin 0
	padding 0
	outline none
body
	font-family: 'AvenirNextCyr', sans-serif
	font-size: 18px
	color: #fff
	background-color: rgb(30,19,81)
	background-image url(../img/main_bg.jpg)
	background-repeat no-repeat
	background-position: center
	background-size: cover
a
	color: #fefefe
	&:focus
		outline: 0
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight normal
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video_wrapper
	margin 0 auto
	position relative
	width 100%
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 3px
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
.button
	display block
	width 100%
	max-width: 250px;
	height: 47px;
	line-height: 47px;
	background-color: rgb(229, 0, 252);
	box-shadow: 0px 3px 7.44px 0.56px rgba(0, 0, 0, 0.35);
	text-align: center
	text-decoration: none
	border-radius: 25px
	margin 0
	transition: all .8s ease-out
	&:focus
		text-decoration: none
		color rgb(255,255,255)
	&:hover
		text-decoration: none
		color rgb(255,255,255)
		box-shadow: 0px 3px 7.44px 0.56px rgba(0, 0, 0, 0.35), 0 0 0 50px rgb(198,0,216) inset
.general_title
	font-size: 29px;
	font-family: 'AvenirNextCyrBold', sans-serif
	line-height: 1.345;
	margin-bottom: 15px
p
li
	line-height: 28px

@import 'main/main'
@import 'main/footer'
@import 'main/medias'